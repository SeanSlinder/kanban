  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "node-pg-migrate": "^7.9.1",
    "nodemon": "^2.0.22"
  }
}
