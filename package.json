{
  "name": "trello-clone",
  "version": "0.1.0",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --exec node src/server.js",
    "migrate:up": "node-pg-migrate up -d $DATABASE_URL -m migrations",
    "migrate:down": "node-pg-migrate down -d $DATABASE_URL -m migrations"
